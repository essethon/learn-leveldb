{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "clang++ build active file",
      "command": "/usr/bin/clang++",
      "args": [
        "-std=c++17",
        "-stdlib=libc++",
        "-I/Users/essethon/dev/leveldb-doc/leveldb/include",
        "-g",
        "${file}",
        "/Users/essethon/dev/leveldb-doc/leveldb/build/libleveldb.a",
        "-lsnappy",
        "-lpthread",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}.o"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
